<?xml version="1.0" encoding="utf-8" ?>
<decision-coverage>
	<project name="someProject" source="./projectSource" workdir="../coverage/workDir">
		<tests>
			<argument name="--filter" value="" />
		</tests>
		<report>
			<writer format="html" outputdir="../../report/" />
		</report>
	</project>
	<log>
		<!-- "level" is the same as in LogLevel class -->
		<handler type="file" path="file:///tmp/decisioncoverage.log" level="debug" />
	</log>
</decision-coverage>